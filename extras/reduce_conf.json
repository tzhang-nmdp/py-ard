{
  "in_csv_filename": "sample.csv",
  "out_csv_filename": "clean_sample.csv",
  "columns_from_csv": [
    "rid",
    "did",
    "r_a_typ1",
    "r_a_typ2",
    "r_b_typ1",
    "r_b_typ2",
    "r_c_typ1",
    "r_c_typ2",
    "r_drb1_typ1",
    "r_drb1_typ2"
  ],
  "locus_column_mapping": {
    "recipient": {
      "A": [
        "r_a_typ1",
        "r_a_typ2"
      ],
      "B": [
        "r_b_typ1",
        "r_b_typ2"
      ],
      "C": [
        "r_c_typ1",
        "r_c_typ2"
      ],
      "drb1": [
        "r_drb1_typ1",
        "r_drb1_typ2"
      ]
    }
  },

  "redux_type": "lgx",
  "redux_cache_size": 8000,

  "reduce_serology": false,
  "reduce_v2": true,
  "convert_v2_to_v3": false,
  "reduce_2field": true,
  "reduce_3field": true,
  "reduce_P": true,
  "reduce_XX": false,
  "reduce_MAC": false,
  "map_drb345_to_drbx": false,

  "locus_in_allele_name": false,
  "keep_locus_in_allele_name": false,

  "new_column_for_redux": false,
  "reduced_column_prefix": "reduced_",

  "generate_glstring": false,

  "output_file_format": "csv",
  "apply_compression": "gzip",

  "verbose_log": true
}
